{
    "name": "solid-element-ui",
    "version": "0.1.15",
    "description": "A Component Library for solidjs",
    "type": "module",
    "files": [
        "dist",
        "README.md",
        "index.tsx",
        "src"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "browser": "./dist/index.js"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "preview": "vite preview"
    },
    "peerDependencies": {
        "solid-js": ">=1.9.10"
    },
    "devDependencies": {
        "@solid-element-ui/components": "workspace:*",
        "@solid-element-ui/utils": "workspace:*"
    },
    "license": "MIT",
    "author": "sanguogege",
    "keywords": [
        "solid-element-ui",
        "element",
        "component library",
        "ui",
        "solidjs"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sanguogege/solid-element-ui"
    },
    "homepage": "https://github.com/sanguogege/solid-element-ui",
    "bugs": {
        "url": "https://github.com/sanguogege/solid-element-ui/issues"
    },
    "sideEffects": false
}
