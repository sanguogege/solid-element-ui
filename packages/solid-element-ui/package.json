{
    "name": "solid-element-ui",
    "version": "0.1.17",
    "description": "A Component Library for solidjs",
    "type": "module",
    "files": [
        "dist",
        "README.md",
        "index.tsx",
        "src"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "development": "./index.tsx",
            "solid": "./index.tsx",
            "types": "./dist/index.d.ts",
            "browser": "./dist/index.js",
            "default": "./index.tsx",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build"
    },
    "peerDependencies": {
        "solid-js": "^1.9.10"
    },
    "dependencies": {
        "class-variance-authority": "^0.7.1",
        "@floating-ui/dom": "^1.7.4"
    },
    "devDependencies": {
        "@solid-element-ui/utils": "workspace:*",
        "@tailwindcss/vite": "*"
    },
    "license": "MIT",
    "author": "sanguogege",
    "keywords": [
        "solid-element-ui",
        "element",
        "component library",
        "ui",
        "solidjs"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sanguogege/solid-element-ui"
    },
    "homepage": "https://github.com/sanguogege/solid-element-ui",
    "bugs": {
        "url": "https://github.com/sanguogege/solid-element-ui/issues"
    },
    "sideEffects": false,
    "include": [
        "index.tsx",
        "**/*"
    ]
}
